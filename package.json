	"babel": {
		"presets": [
			"react",
			"node5",
			"stage-0"
		],
		"env": {
			"test": {
				"plugins": [
					"rewire"
				]
			}
		}
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"babel"
		],
		"extends": "google",
		"globals": {
			"__DEV__": true
		},
		"env": {
			"browser": true
		},
		"rules": {
			"generator-star-spacing": "off",
			"new-cap": "off",
			"max-len": "off",
			"array-bracket-spacing": "off",
			"object-curly-spacing": "off",
			"object-shorthand": "off",
			"arrow-parens": "off",
			"comma-dangle": [
				"error",
				"never"
			],
			"import/no-extraneous-dependencies": "off",
			"react/forbid-prop-types": "off",
			"react/jsx-filename-extension": "off",
			"react/no-danger": "off",
			"react/no-unused-prop-types": "off",
			"react/require-extension": "off",
			"babel/generator-star-spacing": 1,
			"babel/new-cap": "off",
			"babel/array-bracket-spacing": [
				"error",
				"always"
			],
			"babel/object-curly-spacing": "off",
			"babel/object-shorthand": 1,
			"babel/arrow-parens": "off"
		}
	},
	"stylelint": {
		"extends": "stylelint-config-standard",
		"rules": {
			"string-quotes": "single",
			"property-no-unknown": [
				true,
				{
					"ignoreProperties": [
						"composes"
					]
				}
			],
			"selector-pseudo-class-no-unknown": [
				true,
				{
					"ignorePseudoClasses": [
						"global",
						"local"
					]
				}
			]
		}
	},
	"scripts": {
		"eslint": "eslint src tools",
		"stylelint": "stylelint \"src/**/*.css\"",
		"lint": "npm run eslint && npm run stylelint",
		"test": "mocha \"src/**/*.spec.js\" --require test/setup.js --compilers js:babel-register",
		"test:watch": "npm run test -- --reporter min --watch",
		"clean": "babel-node tools/run clean",
		"copy": "babel-node tools/run copy",
		"extractMessages": "babel-node tools/run extractMessages",
		"bundle": "babel-node tools/run bundle",
		"build": "babel-node tools/run build",
		"deploy": "babel-node tools/run deploy",
		"render": "babel-node tools/run render",
		"start": "babel-node tools/run start",
		"createMigration": "babel-node tools/run createMigration",
		"updateMeasurers": "./tools/update_measurers.sh"
	},
	"deployment": {
		"repository": "git@projects.rtk-sdo.ru:rtk-ibc/core/eor-dist.git",
		"website": "http://eor.biblio.rt.ru"
	}
}
